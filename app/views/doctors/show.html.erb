<h1><%= @doctor.name %></h1>

<ul>
	<% @doctor.appointments.each do |appointment| %>
		<li>
			<%= link_to appointment.patient.name, patient_path(appointment.patient) %> / 
			<%= link_to appointment.appointment_datetime.strftime("%B %e, %Y at %H:%M"), appointment_path(appointment) %>
				
			</li>
	<% end %>
</ul>